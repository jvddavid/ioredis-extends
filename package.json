{
  "name": "@jvddavid/ioredis-extends",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A simple wrapper around ioredis to extend its functionality",
  "keywords": [
    "redis",
    "ioredis",
    "extends",
    "wrapper",
    "plugin",
    "expire",
    "hexpire",
    "expireat",
    "hexpireat",
    "ttl"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "pkgroll": "^2.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "scripts": {
    "prepublishOnly": "npm run type:check && npm run lint && npm run build && npm run test",
    "start": "node dist/index.mjs",
    "type:check": "tsc --noEmit",
    "build": "pkgroll --clean-dist --minify --target=es2023 --target=node20",
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "dev": "tsx --watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "optionalDependencies": {}
}
